{
    "type": "FeatureCollection",
    "name": "DB_Ottawa",
    "crs": {
        "type": "name",
        "properties": {
            "name": "urn:ogc:def:crs:OGC:1.3:CRS84"
        }
    },
    "features":[
        {
            "type": "Feature",
            "properties": {
                "from": "ID of Cell from",
                "to": "ID of cell to",
                "conn": 1,  // connectivity between cells (you can set this to 1 always)
                "mob": 1    // mobility between cells (you can set here the correlation factor of your example)
            }
        }, {
            "type": "Feature",
            "properties": {
                "from": "ID of other cell from",
                "to": "ID of other cell to",
                "conn": 1,
                "mob": 1
            }
        }
    ]
}