{
  "data_sources": [
    {
      "id": "db_ottawa",
      "path": "../data/DB_Ottawa_Simple.geojson"
    }, {
      "id": "vicinities_ottawa",
      "path": "../data/DB_Ottawa_Vicinities_Simple.geojson"
    }
  ],
  "simulation": {
    "model_id": "2_2_agent_sir_config",
    "default_config": {
      "cell_type": "sir",
      "delay": "hybrid",
      "state": {
        "population": 100,
        "susceptible": 0.99,
        "infected": 0.01,
        "recovered": 0
      },
      "config": {
        "virulence": 0.4,
        "recovery": 0.6
      }
    },
    "cells": [
      {
        "source": "db_ottawa",
        "cell_id": "dbuid",
        "state": {
          "population": "id"
        }
      }
    ],
    "vicinities": {
      "source": "vicinities_ottawa",
      "from": "from",
      "to": "to",
      "vicinity": {
        "connectivity": "conn",
        "mobility": "mob"
      }
    },
    "config_output_path": "../data/results/build_simulation_output.json",
    "result_output_path": "../data/results/run_simulation_output.txt"
  }
}
